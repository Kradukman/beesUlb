<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_insect_super_family_form" model="ir.ui.view">
        <field name="name">insect.super.family.form</field>
        <field name="model">insect.super.family</field>
        <field name="arch" type="xml">
            <form string="Super Family Form">
                <sheet>
                    <field name="name"/>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_super_family_tree" model="ir.ui.view">
        <field name="name">insect.super.family.tree</field>
        <field name="model">insect.super.family</field>
        <field name="arch" type="xml">
            <tree string="Super Family Tree" multi_edit="1" import="false">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="super_family_search_view" model="ir.ui.view">
        <field name="name">insect.super.family.search</field>
        <field name="model">insect.super.family</field>
        <field name="arch" type="xml">
            <search string="Super Family">
                <field name="name" string="Name" />
            </search>
        </field>
    </record>
</odoo>
