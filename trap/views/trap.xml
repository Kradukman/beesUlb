<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_trap_trap_form" model="ir.ui.view">
        <field name="name">trap.trap.form</field>
        <field name="model">trap.trap</field>
        <field name="arch" type="xml">
            <form string="Trap Form">
                <sheet>
                    <field name="name" />
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_trap_tree" model="ir.ui.view">
            <field name="name">trap.trap.tree</field>
            <field name="model">trap.trap</field>
            <field name="arch" type="xml">
                <tree string="Trap Tree">
                    <field name="name"/>
                </tree>
            </field>
        </record>

</odoo>
