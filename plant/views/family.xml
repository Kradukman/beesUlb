<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_plant_family_form" model="ir.ui.view">
        <field name="name">plant.family.form</field>
        <field name="model">plant.family</field>
        <field name="arch" type="xml">
            <form string="Family Form">
                <sheet>
                    <field name="name" />
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_family_tree" model="ir.ui.view">
            <field name="name">plant.family.tree</field>
            <field name="model">plant.family</field>
            <field name="arch" type="xml">
                <tree string="Family Tree">
                    <field name="name"/>
                </tree>
            </field>
        </record>

</odoo>
