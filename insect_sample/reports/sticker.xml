<?xml version="1.0"?>
<odoo>
    <report
        id="action_report_sticker_print"
        string="Insect sample sticker to print"
        model="insect.sample.sample"
        report_type="qweb-pdf"
        file="insect_sample.report_template_sticker_print"
        name="insect_sample.report_template_sticker_print"
        />

    <record id="insect_sample.action_report_sticker_print" model="ir.actions.report">
        <field name="paperformat_id" ref="base.paperformat_euro"/>
    </record>

     <template id="insect_sample.report_template_sticker_print">
        <t t-call="web.html_container">
            <div class="page">
                <t t-foreach="docs" t-as="o">
                        <t t-esc="o.name"/>
                </t>
            </div>
        </t>
    </template>

</odoo>